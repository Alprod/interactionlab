<div id="card-feed-list-{{ state }}" class="hidden">
    {% if allFeeds is defined and allFeeds is not null %}
        <p class="text-gray-400 dark:text-gray-200">A ce jour vous avez {{ color == 'violet' ? 'envoyer' : 're√ßu' }} {{ allFeeds|length ~ ' Feedback' }}</p>

        <div class="overflow-y-auto max-h-36 lg:max-h-56 md:max-h-56 shadow-inner rounded-lg p-4 not-prose">
            <ul role="list" class="mt-3 grid grid-cols-2 gap-3 sm:gap-4 lg:grid-cols-3 ">
                {% for feedSend in allFeeds %}
                    <li class="w-full bg-slate-100 lg:w-44 col-span-1 flex border-transparent shadow-lg rounded-md dark:bg-slate-600">
                        <div class="flex-1 flex items-center justify-between rounded-lg truncate">
                            <div class="flex-1 px-4 py-2 text-sm truncate prose">
                                <p class="text-gray-900 font-medium dark:text-gray-300">{{ color == 'violet' ? feedSend.username : feedSend.sender }}</p>

                                <div class='cardStart flex flex-row items-center' data-grades-card="{{ feedSend.grade }}">
                                {% for i in 1..5 %}
                                  <div id='{{ 'starCardId_'~  i}}' class='starCard text-yellow-300/10'>
                                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor" class="w-5 h-5 rate_start">
                                      <path fill-rule="evenodd" d="M10.788 3.21c.448-1.077 1.976-1.077 2.424 0l2.082 5.007 5.404.433c1.164.093 1.636 1.545.749 2.305l-4.117 3.527 1.257 5.273c.271 1.136-.964 2.033-1.96 1.425L12 18.354 7.373 21.18c-.996.608-2.231-.29-1.96-1.425l1.257-5.273-4.117-3.527c-.887-.76-.415-2.212.749-2.305l5.404-.433 2.082-5.006z" clip-rule="evenodd" />
                                    </svg>
                                </div>
                                {% endfor %}
                                </div>
                                <p class="text-sm text-gray-500/50 dark:text-white/25">{{ feedSend.gapFeed }}</p>
                            </div>
                        </div>
                    </li>
                {% endfor %}
            </ul>
        </div>
    {% else %}
        <p class="text-gray-400 dark:text-gray-200"> Aucun feedback Envoyer </p>
    {% endif %}
</div>